      <h1 class="flex justify-center title-font sm:text-4xl text-3xl mb-4 font-medium text-gray-900"><%=botid.botname%><% if(botid.certification === 'certified') { %><img src="https://cdn.glitch.com/1be813f1-4233-4b7a-bf44-8a562481d51f%2FCertified.png?v=1625404757585" height="45" width="60"><% } %></h1>
      <p class="mb-8 leading-relaxed"><%=deslong%></p>
      <div class="flex justify-center">
        <a><button class="button-main">Vote</button></a>
        <a href="<%=botid.invite%>" class="underline"><button class="button-main" style="background:none !important ;color:var(--main-bg-color);font-weight:bold;">Invite</button></a>

      </div>
    </div>
